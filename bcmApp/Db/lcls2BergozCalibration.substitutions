#this database contains the records nessecary for the script calibrateBergoz.py 
#to generate calibration coeficents
#

file "lcls2BergozDataPoint.db" { pattern
    {P, POINT_NUM, POINT_VAL, ADC_READING_NUM}
    {$(P), 1, -4.5, 100}
    {$(P), 2, -3.0, 100}
    {$(P), 3, -1, 100}
    {$(P), 4, 0, 100}
    {$(P), 5, 1, 100}
    {$(P), 6, 3.0, 100}
    {$(P), 7, 4.5, 100}
}

