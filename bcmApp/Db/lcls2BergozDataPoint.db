#records that hold the data point from the calibration 

#PS set point
record(ao, "$(P):PS_VOLTAGE$(POINT_NUM)"){
    field(DESC, "Power suply set point")
    field(EGU, "Volts")
    field(VAL, "$(POINT_VAL)")
}

#DVM count
record(ao, "$(P):DMM_READING$(POINT_NUM)"){
    field(DESC, "Actual voltage from the PS")
    field(EGU, "Volts")
    field(FLNK, "$(P):ADC_COUNT$(POINT_NUM)")
}

#ADC count
#compress record to average adc counts
record(compress, "$(P):ADC_COUNT_LST$(POINT_NUM)"){
    field(DESC, "The list of recorded adc counts")
    field(NSAM, "$(NUM_SAMP)")
    field(PINI, "NO")
    field(ALG, "compressALG_N_to_1_Average")
    field(BALG, "LIFO Buffer")
    field(INP, "$(P):CHRG")
    field(FLNK, "$(P):ADC_COUNT$(POINT_NUM)")
    field(SCAN, "Passive")
}

#subroutine that will select the last elemnet from the array
record(acalcout, "$(P):ADC_COUNT$(POINT_NUM)"){
    field(DESC, "The adc count of the last run")
    field(SCAN, "Passive") 
    field(INAA, "$(P):ADC_COUNT_LST$(POINT_NUM)") 
    field(CALC, "aa[0,0]") 
}
